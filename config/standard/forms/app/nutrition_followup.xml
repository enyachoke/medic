<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>child nutrition followup</h:title>
    <model>
      <instance>
        <nutrition_followup delimiter="#" id="nutrition_followup" prefix="J1!nutrition_followup!" version="2019-04-08 09-46">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <patient_id/>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_name/>
          <patient_id/>
          <attend_visit>yes</attend_visit>
          <treatment_program/>
          <measurements>
            <gender/>
            <weight/>
            <height/>
            <muac/>
            <wfh/>
            <nn3/>
            <wfh_message/>
            <wfh_note/>
            <exit/>
            <n/>
          </measurements>
          <summary>
            <n_1/>
            <n_2/>
            <n_3/>
            <n_4/>
            <n_5/>
            <n_6/>
          </summary>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </nutrition_followup>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/nutrition_followup/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/nutrition_followup/inputs/source" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/source_id" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/nutrition_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/patient_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/nutrition_followup/patient_uuid" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/nutrition_followup/patient_name" type="string"/>
      <bind calculate="../inputs/contact/patient_id" nodeset="/nutrition_followup/patient_id" type="string"/>
      <bind nodeset="/nutrition_followup/attend_visit" relevant=" /nutrition_followup/inputs/source  = 'task'" type="select1"/>
      <bind calculate="instance('contact-summary')/context/treatment_program" nodeset="/nutrition_followup/treatment_program" type="string"/>
      <bind nodeset="/nutrition_followup/measurements" relevant=" /nutrition_followup/attend_visit  = 'yes'"/>
      <bind nodeset="/nutrition_followup/measurements/gender" required="true()" type="select1"/>
      <bind constraint=". &gt;= 0.8 and . &lt;= 68.5" jr:constraintMsg="Weight should be between 0.8 kg and 68.5 kg" nodeset="/nutrition_followup/measurements/weight" required="true()" type="decimal"/>
      <bind constraint=". &gt;= 45 and . &lt;= 120" jr:constraintMsg="Height should be between 45 cm and 120 cm" nodeset="/nutrition_followup/measurements/height" required="true()" type="decimal"/>
      <bind constraint=". &gt;= 5 and . &lt;= 30" jr:constraintMsg="MUAC should be between 5 and 30 cm" nodeset="/nutrition_followup/measurements/muac" required="false()" type="decimal"/>
      <bind calculate="round(z-score('weight-for-height',  /nutrition_followup/measurements/gender ,  /nutrition_followup/measurements/height ,  /nutrition_followup/measurements/weight ), 1)" nodeset="/nutrition_followup/measurements/wfh" type="string"/>
      <bind nodeset="/nutrition_followup/measurements/nn3" readonly="true()" relevant=" /nutrition_followup/measurements/weight  != '' and  /nutrition_followup/measurements/height  != ''" type="string"/>
      <bind calculate="if( /nutrition_followup/measurements/wfh  = &quot;&quot;, &quot;&quot;, if( ( /nutrition_followup/measurements/wfh  &lt; -3), &quot;&lt; -3SD [Severely Wasted]&quot;, if( ( /nutrition_followup/measurements/wfh  &lt; -2 and  /nutrition_followup/measurements/wfh  &gt;= -3), &quot;-3SD to &lt;-2SD [Moderately Wasted]&quot;, if( ( /nutrition_followup/measurements/wfh  &lt;= 2 and  /nutrition_followup/measurements/wfh  &gt;= -2), &quot;-2SD to 2SD [Normal]&quot;, if( ( /nutrition_followup/measurements/wfh  &gt; 2), &quot;&gt;2SD [Overweight]&quot;, &quot;&quot; ) ) ) ))" nodeset="/nutrition_followup/measurements/wfh_message" type="string"/>
      <bind nodeset="/nutrition_followup/measurements/wfh_note" readonly="true()" relevant=" /nutrition_followup/measurements/weight  != '' and  /nutrition_followup/measurements/height  != ''" type="string"/>
      <bind nodeset="/nutrition_followup/measurements/exit" required="true()" type="select1"/>
      <bind nodeset="/nutrition_followup/measurements/n" readonly="true()" relevant=" /nutrition_followup/measurements/exit  = 'yes'" type="string"/>
      <bind nodeset="/nutrition_followup/summary" relevant=" /nutrition_followup/attend_visit  = 'yes'"/>
      <bind nodeset="/nutrition_followup/summary/n_1" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_2" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_3" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_4" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_5" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_6" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/nutrition_followup/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/nutrition_followup/inputs">
      <label>Patient</label>
      <input appearance="hidden" ref="/nutrition_followup/inputs/source">
        <label>Source</label>
      </input>
      <input appearance="hidden" ref="/nutrition_followup/inputs/source_id">
        <label>Source ID</label>
      </input>
      <group ref="/nutrition_followup/inputs/contact">
        <label>Contact</label>
        <input appearance="db-object" ref="/nutrition_followup/inputs/contact/_id">
          <label>What is the child's name?</label>
          <hint>Select a person from list</hint>
        </input>
      </group>
    </group>
    <select1 ref="/nutrition_followup/attend_visit">
      <label>Did <output value=" /nutrition_followup/patient_name "/> attend the visit?</label>
      <item>
        <label>Yes</label>
        <value>yes</value>
      </item>
      <item>
        <label>No</label>
        <value>no</value>
      </item>
    </select1>
    <group appearance="field-list" ref="/nutrition_followup/measurements">
      <label>Measurements</label>
      <select1 ref="/nutrition_followup/measurements/gender">
        <label>Gender</label>
        <item>
          <label>Male</label>
          <value>male</value>
        </item>
        <item>
          <label>Female</label>
          <value>female</value>
        </item>
      </select1>
      <input ref="/nutrition_followup/measurements/weight">
        <label>Weight (kgs)</label>
      </input>
      <input ref="/nutrition_followup/measurements/height">
        <label>Height (cms)</label>
      </input>
      <input ref="/nutrition_followup/measurements/muac">
        <label>MUAC Measurement</label>
      </input>
      <input ref="/nutrition_followup/measurements/nn3">
        <label>WFH: <output value=" /nutrition_followup/measurements/wfh "/></label>
      </input>
      <input ref="/nutrition_followup/measurements/wfh_note">
        <label>interpretation: <output value=" /nutrition_followup/measurements/wfh_message "/></label>
      </input>
      <select1 ref="/nutrition_followup/measurements/exit">
        <label>Do you want to exit <output value=" /nutrition_followup/patient_name "/> from <output value=" /nutrition_followup/treatment_program "/> treatment program today?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/nutrition_followup/measurements/n">
        <label>Please remember to fill out the nutrition exit form.</label>
      </input>
    </group>
    <group appearance="field-list summary" ref="/nutrition_followup/summary">
      <label></label>
      <input ref="/nutrition_followup/summary/n_1">
        <label>&lt;h4 style=&quot;text-align:center;&quot;&gt;<output value=" /nutrition_followup/patient_name "/>&lt;/h4&gt;</label>
      </input>
      <input ref="/nutrition_followup/summary/n_2">
        <label>Gender: <output value=" /nutrition_followup/measurements/gender "/></label>
      </input>
      <input ref="/nutrition_followup/summary/n_3">
        <label>Weight: <output value=" /nutrition_followup/measurements/weight "/> kg</label>
      </input>
      <input ref="/nutrition_followup/summary/n_4">
        <label>Height: <output value=" /nutrition_followup/measurements/height "/> cm</label>
      </input>
      <input ref="/nutrition_followup/summary/n_5">
        <label>Weight for height z-score: <output value=" /nutrition_followup/measurements/wfh "/></label>
      </input>
      <input ref="/nutrition_followup/summary/n_6">
        <label>To be exited from <output value=" /nutrition_followup/treatment_program "/> treatment program: <output value=" /nutrition_followup/measurements/exit "/></label></input>
    </group>
  </h:body>
</h:html>
